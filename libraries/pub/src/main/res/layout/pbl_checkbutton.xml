<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_layout_2" >
    
    <Button
        android:enabled="false"
        android:id="@+id/checkbutton_left"
        android:layout_width="64dp"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:background="@drawable/style_bt_check"
        android:textColor="@android:color/white"
        android:textSize="@dimen/app_text_14"
        android:text="业主" />
    
    <Button
        android:id="@+id/checkbutton_right"
        android:layout_width="64dp"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true"
        android:background="@drawable/style_bt_check"
        android:textColor="@color/app_text_3"
        android:textSize="@dimen/app_text_14"
        android:text="租户" />
    
</RelativeLayout>
